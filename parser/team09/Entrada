Select * from tabla1;
Select id1, id2, id3 from tabla2;
select id1 from tabla1, tabla2;
select * from tabla1 order by id1 asc;
select * from tabla2 order by id2 desc;
select * from tabla1 where id1 = 1;
select id1, id2 from tabla1 where id1 = 1 order by id2 asc;
select * from t1 where d1 = d2 and d3 = 1;
select d1 from tabla join tabla2;
select * from tabla join tabla2 on td1 = td2;
select * from tabla join tabla2 inner join tabla3;
select * from tabla join tabla2 on td1 = td2 where d1 = 2;
select * from tabla join tabla2 on td1 = td2 where d1 = 2 order by d3 asc;
